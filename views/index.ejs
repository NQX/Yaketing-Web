      <!-- Contact -->
      <section id="contact">
        <div class="container">
          <div class="row">
            <div class="col-lg-12 text-center">
              <h2 class="section-heading text-uppercase">Contact Us</h2>
              <h3 class="contact-subtext">
                Ihre Probleme sind auch unsere. Wir sind 24/7 für Sie da.
              </h3>
            </div>
          </div>
          <div class="row">
            <div class="col-lg-12">
              <form id="contactForm" name="sentMessage" novalidate="novalidate">
                <div class="row">
                  <div class="col-md-6">
                    <div class="form-group">
                      <input class="form-control contact-input-sm" id="name" type="text" placeholder="Your Name *" required="required" data-validation-required-message="Bitte geben Sie einen Namen an.">
                      <p class="help-block text-danger"></p>
                    </div>
                    <div class="form-group">
                      <input class="form-control contact-input-sm" id="email" type="email" placeholder="Your Email *" required="required" data-validation-required-message="Bitte geben Sie eine Email Adresse an.">
                      <p class="help-block text-danger"></p>
                    </div>
                    <div class="form-group">
                      <input class="form-control contact-input-sm" id="phone" type="tel" placeholder="Your Phone">
                      <p class="help-block text-danger"></p>
                    </div>
                  </div>
                  <div class="col-md-6">
                    <div class="form-group">
                      <textarea class="form-control contact-input-lg" id="message" placeholder="Your Message *" required="required" data-validation-required-message="Bitte geben Sie eine Nachricht ein."></textarea>
                      <p class="help-block text-danger"></p>
                    </div>
                  </div>
                  <div class="clearfix"></div>
                  <div class="col-lg-12 text-center">
                    <div id="success"></div>
                    <button id="sendMessageButton" class="btn btn-primary btn-contact text-uppercase" type="submit">Send Message</button>
                  </div>
                </div>
              </form>
            </div>
          </div>
        </div>
      </section>


